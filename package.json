{
  "name": "vendure-internal-plugin",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "private": true,
  "scripts": {
    "watch": "concurrently -n tsc,gulp \"tsc --watch\" \"gulp watch\"",
    "build": "rimraf lib && tsc && gulp build",
    "dev": "concurrently yarn:dev:*",
    "typecheck": "tsc",
    "dev:server": "ts-node-dev ./dev/server/index.ts",
    "dev:worker": "ts-node-dev ./dev/server/index-worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:e2e": "jest --preset=\"ts-jest\"",
    "generate-types": "graphql-codegen --config ./codegen.json",
    "populate": "yarn ts-node ./dev/server/populate.ts",
    "create-db": "yarn ts-node ./dev/create-db.ts",
    "setup": "yarn create-db && yarn populate",
    "lint": "yarn eslint .",
    "checks": "scripts/checks"
  },
  "resolutions": {
    "@vendure/admin-ui": "2.0.0-beta.2",
    "@vendure/common": "2.0.0-beta.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript-compatibility": "^2.1.5",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vendure/admin-ui": "2.0.0-beta.2",
    "@vendure/admin-ui-plugin": "2.0.0-beta.2",
    "@vendure/asset-server-plugin": "2.0.0-beta.2",
    "@vendure/core": "2.0.0-beta.2",
    "@vendure/create": "2.0.0-beta.2",
    "@vendure/testing": "2.0.0-beta.2",
    "@vendure/ui-devkit": "2.0.0-beta.2",
    "concurrently": "^6.3.0",
    "eslint": "^8.39.0",
    "gulp": "^4.0.2",
    "jest": "^27.3.0",
    "pg": "^8.7.1",
    "pgtools": "^0.3.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {}
}
