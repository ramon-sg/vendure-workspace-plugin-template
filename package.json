{
  "name": "vendure-internal-plugin",
  "version": "0.0.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "private": true,
  "scripts": {
    "watch": "concurrently -n tsc,gulp \"tsc --watch\" \"gulp watch\"",
    "build": "rimraf lib && tsc && gulp build",
    "dev": "concurrently yarn:dev:*",
    "typecheck": "tsc",
    "dev:server": "ts-node-dev ./dev/server/index.ts",
    "dev:worker": "ts-node-dev ./dev/server/index-worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:e2e": "jest --config ./e2e/config/jest-e2e.json",
    "generate-types": "graphql-codegen --config ./codegen.json",
    "populate": "yarn ts-node ./dev/server/populate.ts",
    "create-db": "yarn ts-node ./dev/create-db.ts",
    "setup": "yarn create-db && yarn populate"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript-compatibility": "^2.1.5",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vendure/admin-ui": "^1.0.0",
    "@vendure/admin-ui-plugin": "^1.0.0",
    "@vendure/asset-server-plugin": "^1.0.0",
    "@vendure/core": "^1.0.0",
    "@vendure/create": "^1.0.0",
    "@vendure/testing": "^1.0.0",
    "@vendure/ui-devkit": "^1.0.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "jest": "^27.3.0",
    "pg": "^8.7.1",
    "pgtools": "^0.3.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.3.5"
  },
  "dependencies": {}
}